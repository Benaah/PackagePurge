{
  "name": "packagepurge",
  "version": "1.0.1",
  "description": "Intelligent package manager cache cleanup service with project-aware optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "purge": "dist/cli/index.js",
    "packagepurge": "dist/cli/index.js"
  },
  "scripts": {
    "build": "npm run build:core && tsc",
    "build:core": "cd core && cargo build --release",
    "build:core:debug": "cd core && cargo build",
    "dev": "npm run build:core:debug && ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "clean:core": "cd core && cargo clean"
  },
  "keywords": [
    "npm",
    "yarn",
    "pnpm",
    "cache",
    "cleanup",
    "optimization",
    "storage",
    "deduplication"
  ],
  "author": "Eng. Onyango Benard",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "fs-extra": "^11.2.0",
    "tar": "^6.2.1",
    "glob": "^10.3.10",
    "semver": "^7.5.4",
    "@types/node": "^20.10.6",
    "node-machine-id": "^1.1.12",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.10.6",
    "@types/semver": "^7.5.4",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}